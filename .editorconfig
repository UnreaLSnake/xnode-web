# editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 3
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.json]
indent_size = 2

[.stylelintrc]
indent_size = 2

[*.lock]
indent_size = 2

[*.yml]
indent_size = 2

[*.webmanifest]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

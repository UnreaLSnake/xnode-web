<head>
   <!--general /-->
   <meta charset='UTF-8' />
   {# <base href='https://domain.com/' /> #}
   <meta name='viewport' content='width=device-width, initial-scale=1, shrink-to-fit=no' />

   <!--about /-->
   <title>{{ pageTitle }} – {{ projectName }}</title>
   <meta name='description' content='{{ pageDesc }}' />
   <meta name='robots' content='index, follow' />
   <meta name='apple-mobile-web-app-title' content='{{ projectName }}' />
   <meta name='theme-color' content='#000000' />

   {% if openGraph == 'disable' %}
   {% else %}

      <!--og-global /-->
      <meta property='og:title' content='{{ pageTitle }} – {{ projectName }}' />
      <meta property='og:description' content='{{ pageDesc }}' />
      <meta property='og:type' content='{{ openGraph }}' />
      <meta property='og:url' content='https://domain.com/{{ pageURI }}' />
      <meta property='og:site_name' content='{{ projectName }}' />
      <meta property='og:locale' content='en' />

      {# <!--og-image /-->
      <meta property='og:image' content='https://domain.com/assets/og-image/{{ pageURI }}.jpg' />
      <meta property='og:image:type' content='image/jpeg' />
      <meta property='og:image:width' content='1200' />
      <meta property='og:image:height' content='630' />
      <meta property='og:image:alt' content='{{ pageTitle }}' /> #}

   {% endif %}

   <!--assets /-->
   <link rel='manifest' href='/assets/app.webmanifest' />
   <link
      rel='preload' href='/src/assets/fonts/fontname.woff2'
      as='font' type='font/woff2' crossorigin='anonymous'
   />
   <link rel='stylesheet' href='/src/styles.scss' />
   <script type='module' src='/src/scripts.js'></script>

   <!--optimize /-->
   {# <link rel='preconnect' href='https://domain.com/' /> #}
   {% block preload %}{% endblock %}
   <link rel='canonical' href='https://domain.com/{{ pageURI }}' />

   <!--favicons /-->
   <link rel='icon' href='/assets/a-icons/favicon-96.png' type='image/png' sizes='96x96' />
   <link rel='icon' href='/assets/a-icons/favicon.svg' type='image/svg+xml' />
   <link rel='shortcut icon' href='/assets/a-icons/favicon.ico' />
   <link rel='apple-touch-icon' href='/assets/a-icons/apple-icon.png' sizes='180x180' />

   <!--generated /-->
</head>
